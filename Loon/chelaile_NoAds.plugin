# Web广告API - 返回空JSON对象
^https?:\/\/web\.chelaile\.net\.cn\/api\/adpub\/ad reject-dict

# CDN广告资源 - 返回空响应
^https?:\/\/cdn\.\w{3}\.chelaileapp\.cn\/adpub reject-200

# CDN API广告 - 返回空JSON对象
^https?:\/\/cdn\.\w{3}\.chelaileapp\.cn\/api\/adpub reject-dict

[MITM]
hostname = pic1.chelaile.net.cn, api.chelaile.net.cn, atrace.chelaile.net.cn, web.chelaile.net.cn, *.chelaileapp.cn
