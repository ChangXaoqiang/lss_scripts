[Rewrite]
# 图片广告 - 返回空图片
^https?:\/\/pic1\.chelaile\.net\.cn\/adv\/ reject-img

# API广告请求 - 返回空JSON对象
^https?:\/\/api\.chelaile\.net\.cn\/adpub\/ reject-dict

# 城市广告 - 返回空JSON对象
